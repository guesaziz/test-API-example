meta {
  name: create user valide
  type: http
  seq: 1
}

post {
  url: {{base_url}}/api/users
  body: json
  auth: none
}

params:query {
  ~page: 1
}

body:json {
  {
        "first_name": "{{first_name}}",
        "last_name": "{{last_name}}"
    
  }
}

vars:post-response {
  user-id: res.body.id
}

assert {
  res.status: eq 201
  res.body.first_name: eq {{first_name}}
  res.body.id: isDefined
  res.body.createdAt: contains {{current_date}}
}

settings {
  encodeUrl: true
  timeout: 0
}
